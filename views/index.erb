<script>
    $(document).ready(function(){

        setInterval(function(){$.get('/memory', function(res){
                console.log();
                $('#memory').html(res.memory);
                $('#peak').html(res.peak);
                $('#trough').html(res.trough);
        })}, 500);
    });
</script>


<dl>
